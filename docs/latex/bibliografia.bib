@book{ book:Inversor_inteligente,
	title = {El inversor inteligente},
    isbn = {9788423413027},
    abstract = {Seminal work in the field of value investing, offering timeless principles for successful investing in the stock market. First published in 1949, the book has remained relevant through numerous market cycles and economic changes.

At its core, Graham's philosophy emphasizes the importance of investing with a margin of safety, wherein investors purchase stocks trading at prices below their intrinsic value to mitigate risk. He distinguishes between two types of investors: the "defensive investor," who seeks to preserve capital and achieve steady returns through a diversified portfolio of low-cost, high-quality stocks, and the "enterprising investor," who is willing to devote more time and effort to selecting individual stocks for potentially higher returns},
    language = {Spanish},
    publisher = {Deusto, Grupo Planeta},
    author = {Graham, Benjamin},
    edition = {10th},
    month = sep,
    year = {2007},
    originalyear = {1949}
}

